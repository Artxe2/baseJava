<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper     PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"     "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mybatis_sql">
	<select id="select" parameterType="map" resultType="java.util.Map"> 
		SELECT *
		FROM table
		<if test="1=1">
			<where>
				AND 1=1
			</where>
		</if>
	</select>
</mapper>